<section>

<header class="page-header">
<h1>Admin &raquo; All Categories</h1>
</header>

<div class="new-category-button">
  <%= link_to '+ New Category', [:new, :admin, :category], class: 'btn btn-info' %>
</div>

<table class="table table-bordered" >
  <thead>
    <th>Category Name</th>
    <th>Created On</th>
    <th>Last Updated</th>
  </thead>
  <tbody>
    <%= render @categories %>
  </tbody>
</table>